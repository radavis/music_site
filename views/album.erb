<h3>Album: <%= @album %></h1>
<h3>Artist: <%= @artist %></h2>
<h3>Year: <%= @year %></h3>
<h3>Genre: <%= @genre %></h3>
<h3>Image: <img src="<%= @image %>" width="200" height="200"></h3>

<h3>Tracks:</h3>
<ol>
  <% @album_songs.each do |song| %>
    <li>
      <%= song['title']%> -
      <%= seconds_to_time(song['length'].to_i) %>
    </li>
  <% end %>
</ol>

<a href="/">Home</a>
